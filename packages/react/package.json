{
  "name": "@deskofy/react",
  "version": "0.0.5",
  "private": false,
  "description": "Deskofy Core Library",
  "author": "Deskofy",
  "license": "SEE LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/deskofy/deskofy.git"
  },
  "bugs": "https://github.com/deskofy/deskofy/issues",
  "homepage": "https://deskofy.app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "electron",
    "framework",
    "web",
    "server",
    "node",
    "front-end",
    "backend",
    "deskofy",
    "tvative",
    "typescript",
    "javascript"
  ],
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "react:build": "tsc",
    "react:build:watch": "tsc -w",
    "react:types": "tsc --noEmit --skipLibCheck",
    "react:lint": "eslint .",
    "react:fmt": "prettier --write .",
    "react:npm:pack": "npm pack",
    "react:npm:publish": "npm run react:build && npm publish --access public"
  },
  "dependencies": {
    "typescript": "5.9.2",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "19.2.2"
  }
